    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
